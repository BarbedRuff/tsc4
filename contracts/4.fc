    }
    builders = cons(inner, builders);
    if (s.slice_refs()) {
      stack = cons(s~load_ref(), stack);
    }
  }
  builder ans = builders~list_next();
  while (~ builders.is_null()) {
    ans = builders~list_next().store_ref(ans.end_cell());
  }
  return ans.end_cell();
}